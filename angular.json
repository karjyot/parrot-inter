{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "pat": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/robots.txt",
              "src/sitemap.xml"
              
            ],
            "styles": [
              "src/assets/css/font-awesome.min.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/main-style.css",
              "src/assets/css/responsive.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/@ngx-share/button/themes/default/default-theme.css",
              "src/styles.css",
              "node_modules/cookieconsent/build/cookieconsent.min.css",
              "node_modules/@syncfusion/ej2-base/styles/material.css",
              "node_modules/@syncfusion/ej2-icons/styles/material.css",
              "node_modules/@syncfusion/ej2-buttons/styles/material.css",
              "node_modules/@syncfusion/ej2-splitbuttons/styles/material.css",
              "node_modules/@syncfusion/ej2-inputs/styles/material.css",
              "node_modules/@syncfusion/ej2-lists/styles/material.css",
              "node_modules/@syncfusion/ej2-navigations/styles/material.css",
              "node_modules/@syncfusion/ej2-popups/styles/material.css",
              "node_modules/@syncfusion/ej2-richtexteditor/styles/material.css"
            ],
            "scripts": [
              "src/assets/js/jquery.min.js",
              "src/assets/js/bootstrap.bundle.min.js",
              "src/assets/js/coreui.js",
              "src/assets/js/popper.min.js",
              "node_modules/cookieconsent/build/cookieconsent.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "15mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "500kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "pat:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "pat:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "pat:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/robots.txt"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "pat:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "pat:serve:production"
            }
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/server",
            "main": "src/main.server.ts",
            "tsConfig": "tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": {
                "scripts": false,
                "styles": true
              }
            }
          }
        }
      }
    }
  },
  "defaultProject": "pat"
}